@article{JuanGuillermoLazoLazo2004,
abstract = {As decis{\~{o}}es econ{\^{o}}micas de investimento, assim como as avalia{\c{c}}{\~{o}}es econ{\^{o}}micas de projetos, s{\~{a}}o afetadas por incertezas econ{\^{o}}micas, incertezas t{\'{e}}cnicas e por flexibilidades gerenciais embutidas em projetos. Flexibilidades gerenciais d{\~{a}}o liberdade ao gerente para tomar decis{\~{o}}es, tais como investir, expandir, parar temporariamente ou abandonar um determinado projeto. Tais flexibilidades possuem valor e s{\'{o}} a teoria de op{\c{c}}{\~{o}}es reais consegue avali{\'{a}}-las. As op{\c{c}}{\~{o}}es reais permitem considerar, al{\'{e}}m das incertezas, a flexibilidade gerencial, tendo por objetivo maximizar o valor da oportunidade de investimento. Para se determinar o valor de uma op{\c{c}}{\~{a}}o real, normalmente s{\~{a}}o utilizados modelos de {\'{a}}rvores binomiais, diferen{\c{c}}as finitas ou t{\'{e}}cnicas de simula{\c{c}}{\~{a}}o Monte Carlo. Entretanto, os m{\'{e}}todos tradicionais de {\'{a}}rvores binomiais e diferen{\c{c}}as finitas s{\~{a}}o impratic{\'{a}}veis na avalia{\c{c}}{\~{a}}o de op{\c{c}}{\~{o}}es com mais de tr{\^{e}}s fatores de incerteza, enquanto que a simula{\c{c}}{\~{a}}o Monte Carlo tem um custo computacional muito elevado devido ao processo iterativo da simula{\c{c}}{\~{a}}o estoc{\'{a}}stica na amostragem de cada vari{\'{a}}vel. O objetivo deste trabalho {\'{e}} pesquisar uma metodologia computacionalmente vi{\'{a}}vel para determinar o valor de op{\c{c}}{\~{o}}es reais sob diversas incertezas t{\'{e}}cnicas e de mercado. Neste contexto, {\'{e}} feita uma investiga{\c{c}}{\~{a}}o multidisciplinar (l{\'{o}}gica fuzzy, computa{\c{c}}{\~{a}}o evolucion{\'{a}}ria, processos estoc{\'{a}}sticos e op{\c{c}}{\~{o}}es reais) em busca de m{\'{e}}todos alternativos que possam reduzir o tempo computacional e assim facilitar as tomadas de decis{\~{a}}o conseq{\"{u}}entes da simula{\c{c}}{\~{a}}o. Para isto, {\'{e}} proposta a uni{\~{a}}o de v{\'{a}}rias t{\'{e}}cnicas: N{\'{u}}meros Fuzzy para representar determinados tipos de incertezas das quais se desconhece o processo estoc{\'{a}}stico que as modela, processos estoc{\'{a}}sticos para representar as demais incertezas e a simula{\c{c}}{\~{a}}o Monte Carlo para obter uma boa aproxima{\c{c}}{\~{a}}o do valor da op{\c{c}}{\~{a}}o real. Al{\'{e}}m disso, aplicase um algoritmo gen{\'{e}}tico em conjunto com a simula{\c{c}}{\~{a}}o Monte Carlo para aproximar uma regra de decis{\~{a}}o {\'{o}}tima e determinar o valor da op{\c{c}}{\~{a}}o real no caso de se ter v{\'{a}}rias op{\c{c}}{\~{o}}es de investimento em um projeto. A regra ajuda na decis{\~{a}}o entre o investimento imediato em uma das op{\c{c}}{\~{o}}es ou a espera por melhores condi{\c{c}}{\~{o}}es, as quais dependem do estado das incertezas consideradas. O modelo proposto foi avaliado em problemas de op{\c{c}}{\~{a}}o de expans{\~{a}}o e de op{\c{c}}{\~{a}}o de investimento em informa{\c{c}}{\~{a}}o, aplicados na {\'{a}}rea de explora{\c{c}}{\~{a}}o e produ{\c{c}}{\~{a}}o de petr{\'{o}}leo, obtendo os mesmos resultados que as t{\'{e}}cnicas convencionais com uma redu{\c{c}}{\~{a}}o expressiva do custo computacional. A principal contribui{\c{c}}{\~{a}}o deste trabalho {\'{e}} a concep{\c{c}}{\~{a}}o de uma nova metodologia para a determina{\c{c}}{\~{a}}o do valor de op{\c{c}}{\~{o}}es reais na presen{\c{c}}a de incertezas t{\'{e}}cnicas e de mercado, que oferece vantagens em rela{\c{c}}{\~{a}}o aos m{\'{e}}todos convencionais. Os resultados obtidos comprovam que o uso de n{\'{u}}meros fuzzy para representar incertezas das quais se desconhece o processo estoc{\'{a}}stico que as modela, reduz significativamente o tempo computacional. Al{\'{e}}m disso, a metodologia demonstra que a t{\'{e}}cnica de algoritmos gen{\'{e}}ticos {\'{e}} adequada para obter uma regra de decis{\~{a}}o {\'{o}}tima, com uma boa aproxima{\c{c}}{\~{a}}o do valor da op{\c{c}}{\~{a}}o real, quando s{\~{a}}o consideradas v{\'{a}}rias op{\c{c}}{\~{o}}es de investimento.},
author = {{Juan Guillermo Lazo Lazo}},
file = {:C$\backslash$:/Users/Jos{\'{e}} Ricardo/Desktop/TCC/5656{\_}1.PDF:PDF},
keywords = {Algoritmos Gen{\'{e}}ticos,Incertezas T{\'{e}}cnicas,N{\'{u}}meros Fuzzy,Op{\c{c}}{\~{o}}es Reais,Processos Estoc{\'{a}}sticos,Simula{\c{c}}{\~{a}}o Monte Carlo,e Econ{\^{o}}micas},
pages = {190},
title = {{Determina{\c{c}}{\~{a}}o do Valor de Op{\c{c}}{\~{o}}es Reais por Simula{\c{c}}{\~{a}}o Monte Carlo com Aproxima{\c{c}}{\~{a}}o por N{\'{u}}meros Fuzzy e Algoritmos Gen{\'{e}}ticos}},
year = {2004}
}
